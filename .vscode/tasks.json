{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "my debugger gcc",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-Wall",
				"-Wextra",
				"-Werror",
				"-g",
				// "${file}",
				"-I",
				"${workspaceFolder}/philo/philosophers.h",
				"${workspaceFolder}/philo/*.c",
				"-o",
				"${workspaceFolder}/philo/philosophers"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}
